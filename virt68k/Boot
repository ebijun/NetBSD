IMG=liveimage-virt68k-raw-2026-01-20.img

qemu-system-m68k  -machine virt -nographic \
	-m 128m -device virtio-net-device,netdev=net0 \
	-netdev type=user,id=net0,hostfwd=tcp:127.0.0.1:10069-:22 \
	-drive file=pkgsrc.img,if=virtio,index=2 \
	-drive file=${IMG},if=virtio,index=1 \
	-append "root=ld0" \
	-kernel netbsd-GENERIC
