dd if=/dev/rsd0d of=back.img count=512 bs=3813376 progress=5

DIR=tmp`date +%Y%m%d-%H%M`
mkdir $DIR

vnconfig vnd0 back.img
 fsck /dev/vnd0a
 mount /dev/vnd0a /mnt
  for i in dmesg pkginfo dmesg7 dmesg0 version
  do
   cp /mnt/root/${i} ${DIR}/${i} 
  done
 umount /mnt
vnconfig -u vnd0

diff -u ../pkgsrc/pkginfo  ${DIR}/pkginfo |more
